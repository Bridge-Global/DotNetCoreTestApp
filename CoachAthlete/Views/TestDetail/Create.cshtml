@model CoachAthlete.Data.Entities.TestDetailEntity

@{
    ViewData["Title"] = "Create";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Create</h1>

<h4>TestDetail</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="DistanceOrTime" class="control-label"></label>
                <input asp-for="DistanceOrTime" class="form-control" />
                <span asp-validation-for="DistanceOrTime" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="TestHeaderId" class="control-label"></label>
                <select asp-for="TestHeaderId" class="form-control" asp-items="ViewBag.TestHeaderId"></select>
            </div>
            <div class="form-group">
                <label asp-for="AthleteId" class="control-label"></label>
                <select asp-for="AthleteId" class="form-control" asp-items="ViewBag.userID"></select>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index" asp-route-id="@TempData["selectedHeaderId"]">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
